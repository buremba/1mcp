{
  "language": "js",
  "npm": {
    "dependencies": {},
    "lockfile": ""
  },
  "policy": {
    "network": {
      "allowedDomains": [
        "*"
      ],
      "deniedDomains": [],
      "denyIpLiterals": false,
      "blockPrivateRanges": false,
      "maxBodyBytes": 5242880,
      "maxRedirects": 5
    },
    "filesystem": {
      "readonly": [
        "/workspace"
      ],
      "writable": [
        "/tmp",
        "/out"
      ],
      "mounts": [
        {
          "source": ".",
          "target": "/workspace",
          "type": "directory",
          "readonly": true
        }
      ]
    },
    "limits": {
      "timeoutMs": 60000,
      "memMb": 256,
      "stdoutBytes": 1048576
    }
  },
  "mcps": [],
  "sessionTtlMs": 300000,
  "signingKeyPath": ".1mcp/keys/",
  "cacheDir": ".1mcp/capsules/"
}
